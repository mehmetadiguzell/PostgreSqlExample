@model PostgreSqlExample.Models.Student
@{
    ViewData["Title"] = "Update";
}

<form method="post" asp-action="Update">
    <div class="form-group">
        <label>FirstName</label>
        <input type="text" asp-for="FirstName" class="form-control form-text" />
    </div>
    <div class="form-group">
        <label>LastName</label>
        <input type="text" asp-for="LastName" class="form-control form-text" />
    </div>
    <div class="form-group">
        <button type="submit" class="btn btn-success">SAVE</button>
    </div>
</form>

